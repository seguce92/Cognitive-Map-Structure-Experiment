reald=[];;
estd=[];

d=loadjson(json);
names=fieldnames(d);
N=length(names);
for i=1:N
    t = d.(names{i});
    estd = [estd t.distance_estimations(1)];
    reald = [reald t.real_distances(1)];
end;

r = corrcoef(estd, reald);

p=polyfit(estd', reald', 1);
yfit = estd*p(1)+p(2);

SSresid = (reald-yfit);
SStotal = ((length(reald)-1) * var(reald));
rsq = 1 - sum(yresid.^2)/SStotal

subplot(1,2,1)
plot(reald);
hold on;
plot(yfit, 'r');

subplot(1,2,2)
scatter(reald, yfit);